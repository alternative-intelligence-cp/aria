// Sample 7: Prime number check

func:isPrime = *int8(int64:n) {
    if (n < 2) {
        return 0;
    }
    
    int64:i = 2;
    while (i * i <= n) {
        if (n % i == 0) {
            return 0;
        }
        i = i + 1;
    }
    
    return 1;
};

func:main = *int8() {
    result:r1 = isPrime(17);  // 1 (true)
    result:r2 = isPrime(20);  // 0 (false)
    result:r3 = isPrime(97);  // 1 (true)
    return 0;
};
