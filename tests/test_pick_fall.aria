// Test pick with fall() explicit fallthrough
// EXACT spec example from Section 8.3

int8:c = 5;

pick(c) {
   (<9) {
       print(`less than 9`);
       fall(done);
   },
   (>9) {
       print(`greater than 9`);
       fall(done);
   },
   (9) {
       print(`exactly 9`);
       fall(done);
   },
   done:(!) {
       print(`cleanup`);
   }
}
