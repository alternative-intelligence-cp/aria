func:abs_int8 = *int8(int8:value) {
    if (value < 0) {
        return 0 - value;
    }
    return value;
};

func:min_int8 = *int8(int8:a, int8:b) {
    if (a < b) {
        return a;
    }
    return b;
};

func:main = *int8() {
    int8:x = abs_int8(-5);
    int8:y = min_int8(3, 7);
    return 0;
};
