// Test defer statements with LIFO execution order
func:main = int8() {
    print("Start");
    
    defer {
        print("Defer 1 (executes last)");
    }
    
    defer {
        print("Defer 2 (executes first)");
    }
    
    print("Middle");
    
    defer {
        print("Defer 3 (executes second)");
    }
    
    print("End");
    
    *0;
};
