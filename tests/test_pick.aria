// Test file for Aria Pick statement (pattern matching)
// Tests: exact match, comparisons, ranges, wildcard
// Test 1: Pick with exact match
func:test_pick_exact = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (0) { result = 10; },
        (1) { result = 20; },
        (2) { result = 30; },
        (*) { result = -1; }
    }
    return result;
};
// Test 2: Pick with less than
func:test_pick_less_than = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (<5) { result = 1; }
        (<10) { result = 2; }
        (*) { result = 3; }
    }
    return result;
};
// Test 3: Pick with greater than
func:test_pick_greater_than = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (>100) { result = 1; }
        (>50) { result = 2; }
        (>10) { result = 3; }
        (*) { result = 4; }
    }
    return result;
};
// Test 4: Pick with less than or equal
func:test_pick_less_equal = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (<=5) { result = 10; }
        (<=10) { result = 20; }
        (*) { result = 30; }
    }
    return result;
};
// Test 5: Pick with greater than or equal
func:test_pick_greater_equal = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (>=100) { result = 100; }
        (>=50) { result = 50; }
        (>=0) { result = 0; }
        (*) { result = -1; }
    }
    return result;
};
// Test 6: Pick with inclusive range
func:test_pick_range_inclusive = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (0..5) { result = 1; }   // 0, 1, 2, 3, 4, 5
        (6..10) { result = 2; }  // 6, 7, 8, 9, 10
        (11..20) { result = 3; } // 11-20
        (*) { result = 4; }
    }
    return result;
};
// Test 7: Pick with exclusive range
func:test_pick_range_exclusive = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (0...5) { result = 1; }   // 0, 1, 2, 3, 4 (not 5)
        (5...10) { result = 2; }  // 5, 6, 7, 8, 9 (not 10)
        (*) { result = 3; }
    }
    return result;
};
// Test 8: Pick with wildcard default
func:test_pick_wildcard = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (0) { result = 10; }
        (1) { result = 20; }
        (*) { result = 999; }  // Default case
    }
    return result;
};
// Test 9: Pick with multiple exact values
func:test_pick_multi = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (0) { result = 1; }
        (1) { result = 1; }
        (2) { result = 1; }
        (3) { result = 2; }
        (4) { result = 2; }
        (5) { result = 2; }
        (*) { result = 3; }
    }
    return result;
};
// Test 10: Pick without wildcard (testing non-match)
func:test_pick_no_wildcard = int8(int8:value) {
    int8:result = -1;  // Default before pick
    pick (value) {
        (0) { result = 0; }
        (1) { result = 1; }
        (2) { result = 2; }
    }
    return result;  // Will be -1 if no match
};
// Test 11: Pick with nested logic
func:test_pick_nested = int8(int8:outer) {
    int8:result = 0;
    pick (outer) {
        (0) {
            int8:inner = 5;
            pick (inner) {
                (5) { result = 100; }
                (*) { result = -1; }
            }
        }
        (1) {
            int8:inner = 10;
            pick (inner) {
                (10) { result = 200; }
                (*) { result = -1; }
            }
        }
        (*) { result = 0; }
    }
    return result;
};
// Test 12: Pick with complex ranges
func:test_pick_complex = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (0..9) { result = 1; }      // Single digit
        (10..99) { result = 2; }    // Double digit
        (100..127) { result = 3; }  // Triple digit (int8 max is 127)
        (*) { result = 4; }
    }
    return result;
};
// Test 13: Pick determining sign
func:test_pick_sign = int8(int8:value) {
    int8:result = 0;
    pick (value) {
        (<0) { result = -1; }  // Negative
        (0) { result = 0; }    // Zero
        (>0) { result = 1; }   // Positive
    }
    return result;
};
// Module-level test execution
func:main = int8() {
    // Test exact matches
    int8:r1 = test_pick_exact(0);   // Should be 10
    int8:r2 = test_pick_exact(1);   // Should be 20
    int8:r3 = test_pick_exact(99);  // Should be -1 (wildcard)
    
    // Test comparisons
    int8:r4 = test_pick_less_than(3);    // Should be 1
    int8:r5 = test_pick_greater_than(60); // Should be 2
    
    // Test ranges
    int8:r6 = test_pick_range_inclusive(3);  // Should be 1
    int8:r7 = test_pick_range_exclusive(5);  // Should be 2
    
    // Test sign
    int8:r8 = test_pick_sign(-5);  // Should be -1
    int8:r9 = test_pick_sign(0);   // Should be 0
    int8:r10 = test_pick_sign(10); // Should be 1
    
    print("All pick tests complete!");
    return 0;
};
