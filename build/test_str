; ModuleID = 'aria_module'
source_filename = "aria_module"

%result_int32 = type { i8, i32 }

declare void @puts(ptr)

declare void @print(ptr)

define internal void @__aria_module_init() {
entry:
  ret void
}

define internal %result_int32 @strlen(ptr %str) {
entry:
  %len = alloca i32, align 4
  %str1 = alloca ptr, align 8
  store ptr %str, ptr %str1, align 8
  store i32 0, ptr %len, align 4
  br label %while_cond

while_cond:                                       ; preds = %while_body, %entry
  %0 = load ptr, ptr %str1, align 8
  %1 = load i32, ptr %len, align 4
  %elem_ptr = getelementptr i64, ptr %0, i32 %1
  %elem = load i64, ptr %elem_ptr, align 4
  %netmp = icmp ne i64 %elem, 0
  br i1 %netmp, label %while_body, label %while_exit

while_body:                                       ; preds = %while_cond
  %2 = load i32, ptr %len, align 4
  %3 = sext i32 %2 to i64
  %addtmp = add i64 %3, 1
  %4 = trunc i64 %addtmp to i32
  store i32 %4, ptr %len, align 4
  br label %while_cond

while_exit:                                       ; preds = %while_cond
  %5 = load i32, ptr %len, align 4
  %result = alloca %result_int32, align 8
  %err_ptr = getelementptr inbounds %result_int32, ptr %result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_ptr = getelementptr inbounds %result_int32, ptr %result, i32 0, i32 1
  store i32 %5, ptr %val_ptr, align 4
  %result_val = load %result_int32, ptr %result, align 4
  ret %result_int32 %result_val
}

define i64 @main() {
entry:
  call void @__aria_module_init()
  ret i64 0
}
