func:popcount_int8 = *int32(int8:x) {
    int32:count = 0;
    int8:val = x;
    int32:i = 0;
    
    while (i < 8) {
        if ((val & 1) != 0) {
            count = count + 1;
        }
        val = val >> 1;
        i = i + 1;
    }
    
    return count;
};

func:clz_int8 = *int32(int8:x) {
    if (x == 0) {
        return 8;
    }
    
    int32:count = 0;
    int8:mask = 1 << (8 - 1);
    
    while ((x & mask) == 0) {
        count = count + 1;
        mask = mask >> 1;
    }
    
    return count;
};

