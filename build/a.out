}

define i8 @add(i8 %a, i8 %b) {
entry:
  %a1 = alloca i8, align 1
  store i8 %a, ptr %a1, align 1
  %b2 = alloca i8, align 1
  store i8 %b, ptr %b2, align 1
  %0 = load i8, ptr %a1, align 1
  %1 = load i8, ptr %b2, align 1
  %addtmp = add i8 %0, %1
  ret i8 %addtmp
}

