// Test MIN
%macro GEN_MIN 1
func:min_%1 = *%1(%1[]:arr, int64:length) {
    %1:minVal = arr[0];
    int64:i = 1;
    while (i < length) {
        if (arr[i] < minVal) {
            minVal = arr[i];
        }
        i = i + 1;
    }
    return minVal;
};
%endmacro

GEN_MIN(int32)
GEN_MIN(flt32)

func:main = *int8() {
    puts("MIN works!");
    return 0;
};
