; ModuleID = 'aria_module'
source_filename = "aria_module"

%result_int32 = type { i8, i32 }

declare void @puts(ptr)

declare void @print(ptr)

define internal void @__aria_module_init() {
entry:
  ret void
}

define internal %result_int32 @process_data(ptr %data_ptr, i64 %len) {
entry:
  %data_ptr1 = alloca ptr, align 8
  store ptr %data_ptr, ptr %data_ptr1, align 8
  %len2 = alloca i64, align 8
  store i64 %len, ptr %len2, align 4
  %0 = load i64, ptr %len2, align 4
  %result = alloca %result_int32, align 8
  %err_ptr = getelementptr inbounds %result_int32, ptr %result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_cast = trunc i64 %0 to i32
  %val_ptr = getelementptr inbounds %result_int32, ptr %result, i32 0, i32 1
  store i32 %val_cast, ptr %val_ptr, align 4
  %result_val = load %result_int32, ptr %result, align 4
  ret %result_int32 %result_val
}

define internal %result_int32 @get_pointer() {
entry:
  %output = alloca i32, align 4
  %num_ptr = alloca ptr, align 8
  %my_ptr = alloca ptr, align 8
  store i64 0, ptr %my_ptr, align 4
  store i64 0, ptr %num_ptr, align 4
  %0 = load ptr, ptr %my_ptr, align 8
  %calltmp = call %result_int32 @process_data(ptr %0, i64 100)
  store %result_int32 %calltmp, ptr %output, align 4
  %1 = load i32, ptr %output, align 4
  %result = alloca %result_int32, align 8
  %err_ptr = getelementptr inbounds %result_int32, ptr %result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_ptr = getelementptr inbounds %result_int32, ptr %result, i32 0, i32 1
  store i32 %1, ptr %val_ptr, align 4
  %result_val = load %result_int32, ptr %result, align 4
  ret %result_int32 %result_val
}

define internal %result_int32 @__user_main() {
entry:
  %val = alloca i32, align 4
  %calltmp = call %result_int32 @get_pointer()
  store %result_int32 %calltmp, ptr %val, align 4
  %0 = load i32, ptr %val, align 4
  %result = alloca %result_int32, align 8
  %err_ptr = getelementptr inbounds %result_int32, ptr %result, i32 0, i32 0
  store i8 0, ptr %err_ptr, align 1
  %val_ptr = getelementptr inbounds %result_int32, ptr %result, i32 0, i32 1
  store i32 %0, ptr %val_ptr, align 4
  %result_val = load %result_int32, ptr %result, align 4
  ret %result_int32 %result_val
}

define i64 @main() {
entry:
  call void @__aria_module_init()
  %0 = call %result_int32 @__user_main()
  ret i64 0
}
