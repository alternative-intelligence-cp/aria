// Using separate macros for each bit width
%macro GEN_POPCOUNT_8 1
func:popcount_%1 = *int32(%1:x) {
    int32:count = 0;
    %1:val = x;
    int32:i = 0;
    while (i < 8) {
        if ((val & 1) != 0) {
            count = count + 1;
        }
        val = val >> 1;
        i = i + 1;
    }
    return count;
};
%endmacro

GEN_POPCOUNT_8(int8)
GEN_POPCOUNT_8(uint8)

func:main = *int8() {
    return 0;
};
