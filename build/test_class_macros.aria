%include "stdlib.inc"

; Simple test of the class system macros

CLASS Vec2
    FIELD x, f32
    FIELD y, f32
END_CLASS

METHOD length, f32
    return Math.sqrt(self.x * self.x + self.y * self.y);
);

func:main = *int8() {
    f32:x = 3.0;
    f32:y = 4.0;
    
    print("Testing class system");
    
    return 0;
};
