// Test reverse without macro
func:reverse_int32 = *int8(int32[]:arr, int64:length) {
    int64:start = 0;
    int64:end = 0;
    end = length - 1;
    while (start < end) {
        int32:tmp = arr[start];
        arr[start] = arr[end];
        arr[end] = tmp;
        start = start + 1;
        end = end - 1;
    }
    return 0;
};

func:main = *int8() {
    puts("Works!");
    return 0;
};
