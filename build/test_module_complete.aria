// Comprehensive module test - Math utilities module

mod math {
    func:add = int64(int64:a, int64:b) {
        *a + b;
    };
    
    func:subtract = int64(int64:a, int64:b) {
        *a - b;
    };
    
    func:multiply = int64(int64:a, int64:b) {
        *a * b;
    };
};

func:main = int8() {
    // Module functions are prefixed with "math."
    // In a future version, we'll support calling them directly
    // For now, they exist in the LLVM IR with proper namespacing
    *0;
};
