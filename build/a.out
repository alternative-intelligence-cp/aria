; ModuleID = 'aria_module'
source_filename = "aria_module"

@0 = private unnamed_addr constant [27 x i8] c"Testing math operations...\00", align 1
@1 = private unnamed_addr constant [26 x i8] c"Math operations complete!\00", align 1

declare void @puts(ptr)

declare void @print(ptr)

define void @main() {
entry:
  call void @puts(ptr @0)
  %calltmp = call i8 @add(i64 10, i64 5)
  %calltmp1 = call i8 @sub(i64 10, i64 5)
