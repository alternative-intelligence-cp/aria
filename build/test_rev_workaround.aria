// Test reverse with workaround
func:reverse_int32 = *int8(int32[]:arr, int64:length) {
    int64:start = 0;
    int64:end = 0;
    int64:one = 1;
    end = length;
    end = end - one;
    while (start < end) {
        int32:tmp = arr[start];
        arr[start] = arr[end];
        arr[end] = tmp;
        start = start + one;
        end = end - one;
    }
    return 0;
};

func:main = *int8() {
    puts("Works!");
    return 0;
};
