func:popcount_int8 = *int32(int8:x) {
    int32:count = 0;
    int8:val = x;
    int32:i = 0;
    
    while (i < 8) {
        if ((val & 1) != 0) {
            count = count + 1;
        }
        val = val >> 1;
        i = i + 1;
    }
    
    *count;
};

func:main = *int32() {
    int32:result = popcount_int8(7) ? -1;
    print(`Result: &{result}`);
    *0;
};
